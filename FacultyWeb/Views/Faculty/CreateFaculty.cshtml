@model IEnumerable<FacultyWeb.Models.Faculty>

@{
    ViewBag.Title = "Добавление пользователя";
}
<form asp-action="CreateFaculty" asp-controller="Faculty" method="post" enctype="multipart/form-data">
    <p>
        <label>Имя</label>
        <input name="Name" class="form-control" />
    </p>
    <p>
        <label>Аватар</label>
        <input name="Image" type="file" class="form-control" />
    </p>
    <p>
        <input type="submit" value="Добавить" />
    </p>
</form>